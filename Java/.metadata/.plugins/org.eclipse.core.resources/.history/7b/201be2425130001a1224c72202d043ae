package graph;

public class Graph {
	Node[] nodes;

	public Graph(int size) {
		nodes = new Node[size];
		for (int i = 0; i < size; i++) {
			nodes[i] = new Node(i);
		}
	}

	void addEdge(int i1, int i2) {
		Node n1 = nodes[i1];
		Node n2 = nodes[i2];

		if (!n1.adjacent.contains(n2)) {
			n1.adjacent.add(n2);
		}

		if (!n2.adjacent.contains(n1)) {
			n2.adjacent.add(n1);
		}
	}

	void dfs() {
		dfs(0);
	}

	void dfs(int index) {

	}
}
