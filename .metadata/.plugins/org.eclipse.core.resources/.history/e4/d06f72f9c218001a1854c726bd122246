package heap;

public class Heap {
	private final int MAX_ELLEMENT = 200;
	private int[] heap;
	private int heap_size;
	
	public Heap() {
		heap = new int[MAX_ELLEMENT];
		heap[0] = 0;
		heap_size = 0;
	}
	
	public int insert(int element) {
		heap_size++;
		System.out.println(heap_size);
		heap[heap_size] = element;
		
		int parent = heap_size / 2;
		int temp = 0;
		
		while (parent < 1) {
			if (heap[parent] < heap[heap_size]) {
				temp = heap[parent];
				heap[parent] = heap[heap_size];
				heap[heap_size] = temp;
			}else {
				break;
			}
			parent = parent / 2;
		}
		return 0;
	}
	
	public int remove() {
		return 0;
	}
	
	public void printHeap() {
		for (int i = 1; i <= heap_size; i++) {
			System.out.println(i+" "+heap[1]);
		}
	}
}
